1. package com.company;

class Main {

    public static void main(String[] args) {
            int array[] = {1,14,9,5,7,6};
            int sum=0;
            for(int i=0; i<array.length; i++) {
                sum=sum+array[i];                                                еще вывести цифры буквами
            }
            System.out.println(sum);
        }
        }


