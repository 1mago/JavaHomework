5. package com.company;

import java.util.Random;

class Main {
    public static void main(String[] args) {

        Random number = new Random();
        int znachenie = number.nextInt(1000);
        System.out.println("randomnaya cifra : " + " " + znachenie);

        int naoborot = 0;
        int a;

        while (znachenie > 0) {
            a = znachenie % 10;
            naoborot = naoborot * 10;
            naoborot = naoborot + a;
            znachenie = znachenie / 10;
        }
        System.out.println("naoborot : " + "" + naoborot);
        if(znachenie == naoborot) {
            System.out.println("Palindrome"); }
        else {System.out.println("not Palindrome");
        }

        }

    }
